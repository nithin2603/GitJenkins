	<xsd:complexType name="TaskValidationResult">
        <xsd:annotation>
       		<xsd:documentation>This is a single TaskValidationResult object.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="0" name="taskId" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="isValid" type="xsd:boolean"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="errorCode" nillable="true" type="xsd:string"/>			
			<xsd:element maxOccurs="1" minOccurs="0" name="errorMessage" nillable="true" type="xsd:string"/>			
		</xsd:sequence>
	</xsd:complexType>	

