						<xsd:element maxOccurs="1" minOccurs="0" name="userNameForPrivileges" type="xsd:string"/>
						<xsd:element maxOccurs="1" minOccurs="0" name="roleNameForPrivileges" type="xsd:string"/>
					</xsd:choice>
					<xsd:element maxOccurs="unbounded" minOccurs="1" name="resourceUid" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getResourcePrivileges" type="GetResourcePrivilegesRequest"/>
	<xsd:complexType name="ResourcePrivilege">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="resourceUid" type="xsd:string"/>
