			<xsd:extension base="SiperianResponse">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="updateTaskReturn" type="UpdateTaskResponse"/>

	<xsd:complexType name="GetTasksRequest">
		<xsd:complexContent>
			<xsd:extension base="SiperianRequest">
                <xsd:sequence>
				    <xsd:element maxOccurs="unbounded" minOccurs="0" name="taskMetaData" type="TaskMetaDataType"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="overdueOnly" type="xsd:boolean"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="summaryOnly" type="xsd:boolean"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="unassigned" type="xsd:boolean"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="canBeAssignedToUser" type="xsd:boolean"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="dsiName" type="xsd:string"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="disablePaging" type="xsd:boolean"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="searchToken" type="xsd:string"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="firstRecord" type="xsd:int"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="recordsToReturn" type="xsd:int"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="sortCriteria" type="xsd:string"/>
				    <xsd:element maxOccurs="1" minOccurs="0" name="returnTotal" type="xsd:boolean"/>
                </xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getTasks" type="GetTasksRequest"/>
	<xsd:complexType name="GetTasksResponse">
		<xsd:complexContent>
			<xsd:extension base="SiperianResponse">
				<xsd:sequence>
		        	<xsd:choice maxOccurs="1" minOccurs="0">
					    <xsd:element maxOccurs="unbounded" minOccurs="0" name="taskMetaData" type="TaskMetaDataType"/>
					    <xsd:element maxOccurs="unbounded" minOccurs="0" name="taskData" type="TaskDataType"/>
		        	</xsd:choice>
				  <xsd:element name="searchToken" type="xsd:string"/>
				  <xsd:element name="recordCount" type="xsd:int"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getTasksReturn" type="GetTasksResponse"/>

