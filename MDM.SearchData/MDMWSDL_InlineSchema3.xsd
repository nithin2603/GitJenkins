<?xml version="1.0" encoding="UTF-8"?><!--
    project:   Informatica MDM
    copyright: Informatica LLC. (c) 2003-2013.  All rights reserved.

		--><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="urn:siperian.api" xmlns="urn:siperian.api" xmlns:apachesoap="http://xml.apache.org/xml-soap" xmlns:ibmSchExtn="http://www.ibm.com/schema/extensions" xmlns:mdns="urn:metadata.siperian.api" xmlns:ns="urn:siperian.api" xmlns:rns="urn:siperian.api" xmlns:tns="urn:types.siperian.api" xmlns:wsdlsoap="http://schemas.xmlsoap.org/wsdl/soap/">
	<xsd:import namespace="urn:types.siperian.api" schemaLocation="MDMWSDL_InlineSchema1.xsd"/>
	<xsd:import namespace="urn:metadata.siperian.api" schemaLocation="MDMWSDL_InlineSchema2.xsd"/>

	<xsd:complexType name="SiperianRequestFault">
		<xsd:sequence>
			<xsd:element name="requestName" type="xsd:string"/>
			<xsd:element name="errorCode" type="xsd:string"/>
			<xsd:element name="errorMessage" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="SiperianRequestFault" type="SiperianRequestFault"/>
	<xsd:complexType name="SiperianRequestException">
		<xsd:sequence>
			<xsd:element name="requestName" type="xsd:string"/>
			<xsd:element name="errorCode" type="xsd:string"/>
			<xsd:element name="errorMessage" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="SiperianRequestException" type="SiperianRequestException"/>
	
	<xsd:complexType name="Password">
		<xsd:sequence>
			<xsd:element name="password" type="xsd:string"/>
			<xsd:element default="false" name="encrypted" type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="AsynchronousOptions">
		<xsd:sequence>
			<xsd:element default="false" name="isAsynchronous" type="xsd:boolean"/>
			<xsd:element minOccurs="0" name="jmsReplyTo" type="xsd:string"/>
			<xsd:element minOccurs="0" name="jmsCorrelationId" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="SiperianRequest">
        <xsd:annotation>
        	<xsd:documentation>This is the base type for all requests.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="username" nillable="true" type="xsd:string"/>
            <xsd:element minOccurs="0" name="password" nillable="true" type="Password"/>
            <xsd:element minOccurs="0" name="securityPayload" nillable="true" type="xsd:hexBinary"/>
            <xsd:element minOccurs="0" name="orsId" nillable="true" type="tns:OrsId"/>
						<xsd:element minOccurs="0" name="interactionId" nillable="true" type="tns:InteractionId"/>
			<xsd:element minOccurs="0" name="asynchronousOptions" type="AsynchronousOptions"/>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="SiperianObjectRequest">
		<xsd:complexContent>
			<xsd:extension base="SiperianRequest">
				<xsd:sequence>
					<xsd:element name="siperianObjectUid" type="tns:SiperianObjectUid"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SiperianResponse">
        <xsd:annotation>
        	<xsd:documentation>This is the base type for all responses.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element minOccurs="0" name="message" type="xsd:string"/>
			<xsd:element minOccurs="0" name="interactionId" nillable="true" type="tns:InteractionId"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="GBID">
        <xsd:annotation>
        	<xsd:documentation>This is a Global Business Identifier which can be used as a key for a data object. 'name' is the name of the collumn in the data object containing the GBID. 'value' is the GBID value.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="columnUid" type="tns:SiperianObjectUid"/>
			<xsd:element name="value" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="RecordKey">
        <xsd:annotation>
        	<xsd:documentation>This is used to identify a record in a Siperian object (base object, ...). A combination of system name, rowid, source key, GBID can be used to identify a unique record.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
        	<xsd:element minOccurs="0" name="systemName" type="tns:SystemName"/>
        	<xsd:element minOccurs="0" name="rowid" type="tns:Rowid"/>
        	<xsd:element minOccurs="0" name="sourceKey" type="tns:SourceKey"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="periodStartDate" type="xsd:dateTime"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="periodEndDate" type="xsd:dateTime"/>
        	<xsd:element maxOccurs="unbounded" minOccurs="0" name="GBID" type="GBID"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="rowidXref" type="tns:Rowid"/>
        </xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="XrefKey">
        <xsd:annotation>
        	<xsd:documentation>This is used to identify a record in a Siperian Xref object. A combination of system name and source key can be used to identify a unique record.</xsd:documentation>
        </xsd:annotation>
        	<xsd:choice maxOccurs="1" minOccurs="0">
       	        <xsd:sequence>
                    <xsd:element name="systemName" type="tns:SystemName"/>
                    <xsd:element name="sourceKey" type="tns:SourceKey"/>
                    <xsd:element maxOccurs="1" minOccurs="0" name="periodStartDate" type="xsd:dateTime"/>
                    <xsd:element maxOccurs="1" minOccurs="0" name="periodEndDate" type="xsd:dateTime"/>
                </xsd:sequence>
       	        <xsd:element name="rowidXref" type="tns:Rowid"/>
        	</xsd:choice>
	</xsd:complexType>

	<xsd:complexType name="Parameter">
        <xsd:annotation>
        	<xsd:documentation>A single data parameter within a data record - has a value.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
 			<xsd:choice maxOccurs="1" minOccurs="0">
				<xsd:element name="stringValue" type="xsd:string"/>
				<xsd:element name="bigIntegerValue" type="xsd:integer"/>
				<xsd:element name="bigDecimalValue" type="xsd:decimal"/>
				<xsd:element name="dateValue" type="xsd:dateTime"/>
				<xsd:element name="booleanValue" type="xsd:boolean"/>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="Field">
        <xsd:annotation>
        	<xsd:documentation>A single data field within a data record - has a name and a value.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
          <xsd:extension base="Parameter">
	        <xsd:sequence>
				<xsd:element name="name" type="xsd:string"/>
			</xsd:sequence>
		  </xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="TrustOverrideField">
            <xsd:complexContent>
                  <xsd:extension base="Field">
                        <xsd:sequence>
                              <xsd:element name="trustOverride" type="TrustSetting"/>
                        </xsd:sequence>
                  </xsd:extension>
            </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="MatchPathField">
                <xsd:complexContent>
                      <xsd:extension base="Field">
                            <xsd:sequence>
                                  <xsd:element name="matchPathUID" type="xsd:string"/>
                            </xsd:sequence>
                      </xsd:extension>
                </xsd:complexContent>
    </xsd:complexType>

	<xsd:complexType name="TrustEnabledField">
            <xsd:complexContent>
                  <xsd:extension base="Field">
                        <xsd:sequence>
                              <xsd:element minOccurs="0" name="trustScore" type="xsd:float"/>
                              <xsd:element minOccurs="0" name="survivor" type="xsd:boolean"/>
                              <xsd:element minOccurs="0" name="trustSetting" type="TrustSetting"/>
                        </xsd:sequence>
                  </xsd:extension>
            </xsd:complexContent>
    </xsd:complexType>
    
	<xsd:complexType name="BvtField">
            <xsd:complexContent>
                  <xsd:extension base="Field">
                        <xsd:sequence>
                              <xsd:element minOccurs="0" name="rowidLineage" type="tns:Rowid"/>
                              <xsd:element minOccurs="0" name="sourceKey" type="xsd:string"/>
                              <xsd:element minOccurs="0" name="systemName" type="xsd:string"/>
                        </xsd:sequence>
                  </xsd:extension>
            </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="RecordTypes">
      <xsd:sequence>
        <xsd:element default="true" maxOccurs="1" minOccurs="0" name="package" type="xsd:boolean"/>
        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="xref" type="xsd:boolean"/>
        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="pendingXref" type="xsd:boolean"/>
        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="deletedXref" type="xsd:boolean"/>
        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="history" type="xsd:boolean"/>
        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="xrefHistory" type="xsd:boolean"/>
        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="raw" type="xsd:boolean"/>
      </xsd:sequence>
    </xsd:complexType>    

    <xsd:simpleType name="MatchType">
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="AUTO"/>
        <xsd:enumeration value="BOTH"/>
        <xsd:enumeration value="DBFILTERED"/>
        <xsd:enumeration value="NONE"/>
      </xsd:restriction>
    </xsd:simpleType> 

    <xsd:complexType name="Record">
        <xsd:annotation>
            <xsd:documentation>This is a single record of data.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:choice maxOccurs="unbounded" minOccurs="0">
                <xsd:element name="field" type="Field"/>
                <xsd:element name="trustOverrideField" type="TrustOverrideField"/>
                <xsd:element name="trustEnabledField" type="TrustEnabledField"/>
                <xsd:element name="matchPathField" type="MatchPathField"/>
                <xsd:element name="bvtField" type="BvtField"/>
            </xsd:choice>
            <xsd:element name="siperianObjectUid" type="tns:SiperianObjectUid"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="HMRecord">
        <xsd:complexContent>
            <xsd:extension base="Record">
                <xsd:sequence>
                	<xsd:element name="recordKey" type="RecordKey"/>
                	<xsd:element name="aggregatePeriodStartDate"/>
                	<xsd:element name="aggregatePeriodEndDate"/>
                	<xsd:element name="notInEffect" type="xsd:boolean"/>
                	<xsd:element name="lockedByInteractionId" type="xsd:string"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:simpleType name="ActionType">
      <xsd:restriction base="xsd:string">
			<xsd:enumeration value="Insert"/>
			<xsd:enumeration value="Update"/>
			<xsd:enumeration value="Accept as Unique"/>
			<xsd:enumeration value="Merge"/>
			<xsd:enumeration value="Delete"/>
			<xsd:enumeration value="Update XREF"/>
			<xsd:enumeration value="Merge Update"/>
			<xsd:enumeration value="UnMerge"/>
			<xsd:enumeration value="No Action"/>
			<xsd:enumeration value="Pending Insert"/>
			<xsd:enumeration value="Pending Update"/>
			<xsd:enumeration value="Pending Update XREF"/>
			<xsd:enumeration value="Promote Pending XREF"/>
			<xsd:enumeration value="BO set to Delete"/>
			<xsd:enumeration value="XREF set to Delete"/>
			<xsd:enumeration value="BO Delete"/>
			<xsd:enumeration value="XREF Delete"/>
			<xsd:enumeration value="AM Rule Event"/>
      </xsd:restriction>
    </xsd:simpleType> 

    <xsd:simpleType name="RecordState">
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="CONSOLIDATED"/>
        <xsd:enumeration value="UNMERGED"/>
        <xsd:enumeration value="QUEUED_FOR_MATCH"/>
        <xsd:enumeration value="NEWLY_LOADED"/>
        <xsd:enumeration value="ON_HOLD"/>
      </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="LookupField">
        <xsd:annotation>
        	<xsd:documentation>This is used to identify a column in a base object. uid can be use to identify the column and value is for look up.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="uid" type="tns:SiperianObjectUid"/>
			<xsd:element name="value" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:simpleType name="ComparisonOperator">
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="EQUALS"/>
        <xsd:enumeration value="NOT_EQUALS"/>
        <xsd:enumeration value="GREATER_THAN"/>
        <xsd:enumeration value="GREATER_THAN_OR_EQUAL"/>
        <xsd:enumeration value="LESS_THAN"/>
        <xsd:enumeration value="LESS_THAN_OR_EQUAL"/>
        <xsd:enumeration value="LIKE"/>
        <xsd:enumeration value="LIKE_ANYWHERE"/>
        <xsd:enumeration value="LIKE_STARTS_WITH"/>
        <xsd:enumeration value="LIKE_ENDS_WITH"/>
        <xsd:enumeration value="NOT_LIKE"/>
        <xsd:enumeration value="NOT_LIKE_ANYWHERE"/>
        <xsd:enumeration value="NOT_LIKE_STARTS_WITH"/>
        <xsd:enumeration value="NOT_LIKE_ENDS_WITH"/>
      </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="TrustTimeUnit">
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Day"/>
        <xsd:enumeration value="Week"/>
        <xsd:enumeration value="Month"/>
        <xsd:enumeration value="Quarter"/>
        <xsd:enumeration value="Year"/>
      </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="TrustGraphType">
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="Linear"/>
        <xsd:enumeration value="Slow Initial, Rapid Later"/>
        <xsd:enumeration value="Rapid Initial, Slow Later"/>
      </xsd:restriction>
    </xsd:simpleType>

	<xsd:complexType name="TrustSetting">
        <xsd:annotation>
        	<xsd:documentation>This is used to describe trust settings for a column.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="custom" type="xsd:boolean"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="maximumTrust" type="xsd:integer"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="minimumTrust" type="xsd:integer"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="timeUnit" type="TrustTimeUnit"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="maximumTimeUnits" type="xsd:integer"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="graphType" type="TrustGraphType"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TrustGraphData">
        <xsd:annotation>
        	<xsd:documentation>This is used to describe trust settings for a column.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="date" type="xsd:date"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="trustValue" type="xsd:integer"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="MergeHistoryNode">
        <xsd:annotation>
        	<xsd:documentation>This is a node in the merge history tree of a BO record.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="rowid" type="tns:Rowid"/>
			<xsd:element name="mergeDate" type="xsd:dateTime"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="historyNode" type="MergeHistoryNode"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="SiperianObjectRecordKey">
        <xsd:annotation>
        	<xsd:documentation>This is a recordKey plus a siperianObjectUid to identify the entity type.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="recordKey" type="RecordKey"/>
			<xsd:element name="siperianObjectUid" type="tns:SiperianObjectUid"/>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="OneHopFilter">
        <xsd:annotation>
        	<xsd:documentation>This is used to specifies filtering criteria.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="hierarchies" type="xsd:string"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="relTypes" type="xsd:string"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="relDirections" type="xsd:string"/>
			<xsd:element default="true" name="activeRelsOnly" type="xsd:boolean"/>            			
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="EntityGraphFilter">
		<xsd:complexContent>
			<xsd:extension base="OneHopFilter">
				<xsd:sequence>
					<xsd:element name="maximumDepth" type="xsd:integer"/>
					<xsd:element name="maximumBreadth" type="xsd:integer"/>
					<xsd:element name="maximumRelationships" type="xsd:integer"/>
					<xsd:element default="true" minOccurs="0" name="countLeafRelations" type="xsd:boolean"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	
    <xsd:complexType name="EntityFilterCriteria">
        <xsd:annotation>
        	<xsd:documentation>This is used to filter entities by data values.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="entityTypeUid" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="1" name="filterCriteria" type="xsd:string"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="filterParameter" type="Parameter"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="EntityInfo">
        <xsd:annotation>
        	<xsd:documentation>This is used to specifies an entity infomation.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="siperianObjectRecordKey" type="SiperianObjectRecordKey"/>
			<xsd:element name="relationshipCount" type="xsd:integer"/>
			<xsd:element name="traversed" type="xsd:boolean"/>
			<xsd:element name="listNode" type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="XrefResult">
        <xsd:annotation>
        	<xsd:documentation>This is used to provide success/failure information on individual xrefs.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="xrefKey" type="XrefKey"/>
			<xsd:element name="success" type="xsd:boolean"/>
			<xsd:element name="message" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="RecordResult">
        <xsd:annotation>
        	<xsd:documentation>This is used to provide success/failure information on individual records.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
			<xsd:element name="recordKey" type="RecordKey"/>
			<xsd:element name="success" type="xsd:boolean"/>
			<xsd:element name="message" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:simpleType name="HubState">
      <xsd:restriction base="xsd:string">
        <xsd:enumeration value="ACTIVE"/>
        <xsd:enumeration value="PENDING"/>
        <xsd:enumeration value="DELETED"/>
      </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="AuthenticateRequest">
		<xsd:complexContent>
			<xsd:extension base="SiperianRequest"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="authenticate" type="AuthenticateRequest"/>
	<xsd:complexType name="AuthenticateResponse">
		<xsd:complexContent>
			<xsd:extension base="SiperianResponse">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="roleName" type="xsd:string"/>
					<xsd:element name="passwordExpires" nillable="false" type="xsd:boolean"/>
					<xsd:element minOccurs="0" name="passwordExpirationDate" nillable="true" type="xsd:dateTime"/>
					<xsd:element name="externalAuthentication" nillable="false" type="xsd:boolean"/>
					<xsd:element name="administrator" nillable="false" type="xsd:boolean"/>
					<xsd:element minOccurs="0" name="username" nillable="true" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="authenticateReturn" type="AuthenticateResponse"/>

    	<xsd:complexType name="GetOrsListRequest">
		<xsd:complexContent>
			<xsd:extension base="SiperianRequest"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getOrsList" type="GetOrsListRequest"/>
	<xsd:complexType name="GetOrsListResponse">
		<xsd:complexContent>
			<xsd:extension base="SiperianResponse">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="ors" type="mdns:MetaDataOrs"/>					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getOrsListReturn" type="GetOrsListResponse"/>

	<xsd:complexType name="GetRequest">
		<xsd:complexContent>
			<xsd:extension base="SiperianObjectRequest">
				<xsd:sequence>
					<xsd:element name="recordKey" type="RecordKey"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="systemName" type="tns:SystemName"/>
					<xsd:element maxOccurs="1" minOccurs="0" name="recordTypes" type="RecordTypes"/>
					<xsd:element default="false" maxOccurs="1" minOccurs="0" name="returnTrustScores" type="xsd:boolean"/>
					<xsd:element default="false" maxOccurs="1" minOccurs="0" name="returnLineage" type="xsd:boolean"/>
					<xsd:element maxOccurs="1" minOccurs="0" name="effectiveDate" type="xsd:dateTime"/>
					<xsd:element maxOccurs="1" minOccurs="0" name="historyDate" type="xsd:dateTime"/>
					<xsd:element maxOccurs="1" minOccurs="0" name="dataFilter" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="get" type="GetRequest"/>
	<xsd:complexType name="GetResponse">
		<xsd:complexContent>
			<xsd:extension base="SiperianResponse">
				<xsd:sequence>
					<xsd:element name="recordKey" type="RecordKey"/>
			        <xsd:element maxOccurs="unbounded" minOccurs="0" name="record" type="Record"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="lastUpdateDate" type="xsd:dateTime"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="lastUpdatingXref" type="XrefKey"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="aggregatePeriodStartDate" type="xsd:dateTime"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="aggregatePeriodEndDate" type="xsd:dateTime"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="lockedByInteractionId" type="xsd:boolean"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getReturn" type="GetResponse"/>

	<xsd:complexType name="GetXrefForEffectiveDateRequest">
		<xsd:complexContent>
			<xsd:extension base="SiperianObjectRequest">
				<xsd:sequence>
					<xsd:element name="recordKey" type="RecordKey"/>
			        <xsd:element maxOccurs="unbounded" minOccurs="0" name="systemName" type="tns:SystemName"/>
			        <xsd:element maxOccurs="3" minOccurs="0" name="recordState" type="HubState"/>					<xsd:element maxOccurs="1" minOccurs="0" name="effectiveDate" type="xsd:dateTime"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="historyDate" type="xsd:dateTime"/>
			        <xsd:element maxOccurs="1" minOccurs="0" name="dataFilter" type="xsd:string"/>
			        <xsd:element default="false" maxOccurs="1" minOccurs="0" name="otherPeriods" type="xsd:boolean"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getXrefForEffectiveDate" type="GetXrefForEffectiveDateRequest"/>
	
	<xsd:complexType name="GetXrefForEffectiveDateResponse">
		<xsd:complexContent>
			<xsd:extension base="SiperianResponse">
				<xsd:sequence>
					<xsd:element name="recordKey" type="RecordKey"/>
