                    <xsd:element minOccurs="0" name="searchToken" type="xsd:string"/>
                    <xsd:element default="1" minOccurs="0" name="firstRecord" type="xsd:int"/>
                    <xsd:element default="0" minOccurs="0" name="recordsToReturn" type="xsd:int"/>
                    <xsd:element default="true" minOccurs="0" name="returnTotal" type="xsd:boolean"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
