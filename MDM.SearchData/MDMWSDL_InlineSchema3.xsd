	<xsd:complexType name="GetResourcePrivilegesResponse">
		<xsd:complexContent>
			<xsd:extension base="SiperianResponse">
				<xsd:sequence>
					<xsd:element maxOccurs="unbounded" minOccurs="1" name="resourcePrivilege" type="ResourcePrivilege"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element ibmSchExtn:docRoot="true" name="getResourcePrivilegesReturn" type="GetResourcePrivilegesResponse"/>


	<xsd:complexType name="TaskMetaDataType">
        <xsd:annotation>
       		<xsd:documentation>This is a single TaskMetaData object.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" name="workflowProcessId" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="taskId" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="taskType" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="ownerUid" type="xsd:string"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="previousTaskId" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="previousOwnerUid" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="dueDate" type="xsd:dateTime"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="status" type="TaskStatusType"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="priority" type="TaskPriorityType"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="subjectAreaUid" type="xsd:string"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="title" type="xsd:string"/>
			<xsd:element minOccurs="0" name="interactionId" type="tns:InteractionId"/>
            <xsd:element minOccurs="0" name="creator" type="xsd:string"/>
            <xsd:element minOccurs="0" name="createDate" type="xsd:date"/>
            <xsd:element minOccurs="0" name="lastUpdatedBy" type="xsd:string"/>
            <xsd:element minOccurs="0" name="lastUpdateDate" type="xsd:date"/>
        </xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TaskDataType">
		<xsd:complexContent>
			<xsd:extension base="TaskMetaDataType">
				<xsd:sequence>
					<xsd:element maxOccurs="1" minOccurs="0" name="comment" type="xsd:string"/>
					<xsd:element maxOccurs="unbounded" minOccurs="0" name="taskRecord" type="TaskRecordType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="TaskRecordType">
		<xsd:sequence>
			<xsd:element maxOccurs="1" minOccurs="1" name="siperianObjectUid" type="tns:SiperianObjectUid"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="recordKey" type="RecordKey"/>
			<xsd:element maxOccurs="1" minOccurs="0" name="matchRuleUid" type="tns:SiperianObjectUid"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:simpleType name="TaskStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="OPEN"/>
			<xsd:enumeration value="CLOSED"/>
			<xsd:enumeration value="OBSOLETE"/>
            <xsd:enumeration value="ANY"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="TaskPriorityType">
		<xsd:restriction base="xsd:integer">
			<xsd:enumeration value="0"/>
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="-1"/>
            <xsd:enumeration value="100"/>
		</xsd:restriction>
	</xsd:simpleType>

    <xsd:simpleType name="TaskPositionType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="USER"/>
            <xsd:enumeration value="WORKFLOW"/>
        </xsd:restriction>
